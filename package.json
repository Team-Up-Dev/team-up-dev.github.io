{
  "name": "organization-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@octokit/request": "^5.4.12",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-slick": "^0.27.13",
    "slick-carousel": "^1.8.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "format": "prettier --write src/*",
    "prettier-watch": "onchange \"src/*\" \"public/*\" -- prettier --write --ignore-unknown {{changed}}",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "dep-check": "depcheck",
    "npm-check": "npm-check",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-Up-Dev/team-up-dev.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/Team-Up-Dev/team-up-dev.github.io/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://team-up-dev.github.io/",
  "devDependencies": {
    "depcheck": "^1.3.1",
    "gh-pages": "^3.1.0",
    "npm-check": "^5.9.2",
    "onchange": "^7.1.0",
    "prettier": "2.2.1",
    "source-map-explorer": "^2.5.1"
  }
}
